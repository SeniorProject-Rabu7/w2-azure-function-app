{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "dbRead",
      "databaseName": "WordTypes",
      "collectionName": "WordTypes",
      "connectionStringSetting": "CosmosDBConnection",
      "direction": "in"
    },
    {
      "type": "cosmosDB",
      "name": "dbWrite",
      "databaseName": "ToDoList",
      "collectionName": "Items",
      "connectionStringSetting": "CosmosDBConnection",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/word-type/main.js"
}
